{
    "name": "@jadesrochers/geomap",
    "version": "0.2.31",
    "license": "MIT",
    "author": "Jared Desrochers",
    "module": "es/index.js",
    "main": "dist/geomap-umd.js",
    "sideEffects": false,
    "repository": {
        "type": "git",
        "url": "https://github.com/jadesrochers/geomap.git"
    },
    "files": [
        "es/",
        "dist/",
        "README.md",
        "LICENSE"
    ],
    "description": "Components to create feature maps using SVG and a minimal amount of d3",
    "keywords": [
        "react",
        "geographic",
        "map",
        "d3",
        "data",
        "interactive"
    ],
    "dependencies": {
        "@babel/runtime": "^7.9.2",
        "@emotion/core": "^10.0.28",
        "@jadesrochers/legends": "^0.1.12",
        "@jadesrochers/reacthelpers": "^0.1.21",
        "@jadesrochers/selectbox": "^0.1.24",
        "d3-array": "^2.4.0",
        "d3-geo": "^1.11.9",
        "d3-scale": "^3.2.1",
        "ramda": "^0.27.0",
        "topojson-client": "^3.1.0",
        "topojson-server": "^3.0.1"
    },
    "peerdependencies": {
        "react": "^16.8.0",
        "react-dom": "^16.8.0"
    },
    "scripts": {
        "test": "BABEL_ENV=jest jest",
        "build-es": "BABEL_ENV=es6 babel --out-dir es --ignore '**/__tests__/' src",
        "build-umd-webp": "BABEL_ENV=umd webpack --config ./webpack.config.js --display-modules",
        "build-umd-roll": "BABEL_ENV=umd rollup --config ./rollup.config.js",
        "profile": "webpack --config ./webpack.config.js --profile --json > webpack-stats.json",
        "start": "BABEL_ENV=es6  webpack-dev-server --progress --port 3000 --host 127.0.0.1 --inline --config demo/webpack.config.js",
        "packagePatch": "npm run test && npm run build-es && npm run build-umd-roll && npm version patch && npm publish && git push -u origin master",
        "packageMinor": "npm run test && npm run build-es && npm run build-umd-roll && npm version patch && npm publish && git push -u origin master",
        "updatePatch": "npm update && npm run test && npm run build-umd-roll && npm version patch && npm publish && git push -u origin master",
        "CommitandPackage": "./CommitandPackage.sh"
    },
    "devDependencies": {
        "@babel/cli": "^7.8.4",
        "@babel/core": "^7.9.0",
        "@babel/plugin-transform-runtime": "^7.9.0",
        "@babel/preset-env": "^7.9.0",
        "@babel/preset-react": "^7.9.4",
        "babel-loader": "^8.1.0",
        "babel-plugin-emotion": "^10.0.29",
        "css-loader": "^3.4.2",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.2",
        "eslint": "^6.8.0",
        "eslint-config-problems": "^4.0.0",
        "eslint-plugin-react": "^7.19.0",
        "eslint-plugin-react-hooks": "^3.0.0",
        "html-webpack-plugin": "^4.0.4",
        "jest": "^25.2.4",
        "jest-emotion": "^10.0.32",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "rollup": "^2.3.2",
        "rollup-plugin-babel": "^4.4.0",
        "rollup-plugin-commonjs": "^10.1.0",
        "rollup-plugin-filesize": "^6.2.1",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-terser": "^5.3.0",
        "style-loader": "^1.1.3",
        "webpack": "^4.42.1",
        "webpack-cli": "^3.3.11",
        "webpack-dev-server": "^3.10.3"
    }
}
